    }
   },
   "outputs": [],
   "source": [
    "def smoothing(image, epsilon):\n",
    "    n, m = image.shape\n",
    "    # New size of the array\n",
    "    alpha = ceil(n*epsilon)\n",
    "    beta = ceil(m*epsilon)\n",
    "    # New image\n",
    "    n_image = np.zeros((alpha,beta))\n",
    "    # Size of the window\n",
    "    ne = ceil(n/alpha)\n",
    "    me = ceil(m/beta)\n",
    "    # Resizing\n",
    "    for i in range(alpha-1):\n",
    "        for j in range(beta-1):\n",
    "            n_image[i,j] = np.mean(image[i*ne:(i+1)*ne, j*me: (j+1)*me])\n",
    "    return n_image"
   ]
  },
