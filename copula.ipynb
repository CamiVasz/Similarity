    "        den2 =  mvt.multivariate_t.pdf(x=vi,mean = 0, df=df)\n",
    "        return num/(den1*den2)\n",
    "    \n",
    "    def lognormal_density(self, X):\n",
    "        self.g_copula\n",
    "        return self.g_copula.pdf(X)\n",
