    "@numba.njit(parallel=True)\n",
    "def num_empirical(X, Y, n):\n",
    "        XY = np.vstack((X,Y)).T\n",
    "        X.sort()\n",
    "        Y.sort()\n",
    "        e_copula = np.zeros((n, n))\n",
    "        for i in numba.prange(n):\n",
    "            for j in numba.prange(n):\n",
    "                c = np.sum(np.where( (np.sum(np.where((XY <= np.array([X[i], Y[j]])), 1, 0), axis = 1)) == 2, 1, 0))\n",
    "                e_copula[i,j] = c / n\n",
    "        return e_copula"
   ]
