    "# New size of the array\n",
    "alpha = ceil(n*e)\n",
    "beta = ceil(m*e)\n",
    "# New image\n",
    "n_t1 = np.zeros((alpha,beta))\n",
    "# Size of the window\n",
    "ne = ceil(n/alpha)\n",
    "me = ceil(m/beta)"
   ]
